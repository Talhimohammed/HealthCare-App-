<?xml version="1.0" encoding="utf-8"?><!--
  -->

<ScrollView


    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    android:background="@color/hex"
    tools:context=".home">


    <LinearLayout

        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/hex"

        android:orientation="vertical"

        tools:ignore="MissingPrefix">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"

            android:paddingLeft="@dimen/horizontal_dashboard"
            android:paddingTop="@dimen/fab_margin"
            android:paddingRight="@dimen/horizontal_dashboard"


            android:weightSum="100">


            <TextView
                android:layout_width="410dp"
                android:layout_height="70dp"
                android:layout_marginTop="20dp"
                android:layout_weight="40"
                android:fontFamily="@font/days_one"
                android:text="WELCOME"
                android:textColor="@color/colorPrimary1"
                android:textSize="30sp">


            </TextView>

            <com.mikhaellopez.circularimageview.CircularImageView
                android:id="@+id/home_profile_image"
                android:layout_width="100dp"
                android:layout_height="80dp"
                android:layout_marginRight="190dp"
                android:layout_marginTop="8dp"
                android:src="@drawable/image_2" />


        </LinearLayout>


        <TextView
            android:id="@+id/search"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="@dimen/horizontal_dashboard"

            android:layout_marginTop="@dimen/normalPadding"
            android:layout_marginRight="@dimen/horizontal_dashboard"

            android:background="@drawable/container_search"
            android:drawableStart="@drawable/ic_search_black_24dp"

            android:drawableLeft="@drawable/ic_search_black_24dp"

            android:drawablePadding="@dimen/drawable_left"
            android:fontFamily="@font/bariol_regular"

            android:gravity="center_vertical"
            android:paddingLeft="@dimen/normalPadding"
            android:paddingTop="@dimen/vertical_search"
            android:paddingRight="@dimen/normalPadding"
            android:paddingBottom="@dimen/vertical_search"

            android:text="Search Here"
            android:textColor="@color/bottomNavigationTextColor"

            android:textSize="14sp">

        </TextView>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@null">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="5dp"
                android:background="@null">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginTop="80dp"
                    android:background="@color/back_courses"
                    android:orientation="vertical">

                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <LinearLayout

                        android:layout_width="373dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/horizontal_dashboard"
                        android:layout_marginTop="@dimen/normalPadding"
                        android:layout_marginRight="@dimen/horizontal_dashboard"
                        android:gravity="center"
                        android:orientation="horizontal"
                        android:weightSum="2">

                        <androidx.cardview.widget.CardView
                            android:id="@+id/medicalfolder"
                            android:layout_width="163dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginTop="20dp"

                            android:layout_marginRight="20dp"
                            android:layout_marginBottom="20dp"
                            android:background="@drawable/layout"
                            android:minHeight="150dp"
                            app:cardBackgroundColor="#FFF"
                            app:cardCornerRadius="16dp"
                            app:cardElevation="@dimen/card_itemcourse_elevation">

                            <!--    app:cardElevation="10dp"-->

                            <LinearLayout
                                android:layout_width="133dp"
                                android:layout_height="125dp"
                                android:layout_gravity="center"
                                android:orientation="vertical">

                                <ImageView

                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:adjustViewBounds="true"
                                    android:cropToPadding="true"
                                    android:maxWidth="95dp"
                                    android:src="@drawable/images" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:layout_marginBottom="10dp"

                                    android:text="Medical Folder"
                                    android:textSize="17sp"
                                    android:textStyle="bold" />

                            </LinearLayout>


                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:id="@+id/Appm"
                            android:layout_width="163dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginTop="20dp"
                            android:layout_marginRight="20dp"
                            android:layout_marginBottom="20dp"
                            android:background="@drawable/layout"
                            android:minHeight="150dp"
                            app:cardBackgroundColor="#FFF"
                            app:cardCornerRadius="16dp"
                            app:cardElevation="@dimen/card_itemcourse_elevation">

                            <!--    app:cardElevation="10dp"-->

                            <LinearLayout
                                android:id="@+id/app"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:orientation="vertical">

                                <ImageView
                                    android:id="@+id/button16"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:adjustViewBounds="true"
                                    android:cropToPadding="true"
                                    android:maxWidth="95dp"
                                    android:src="@drawable/checkup" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:layout_marginBottom="10dp"

                                    android:text="Appoinement"
                                    android:textSize="16sp"
                                    android:textStyle="bold" />

                            </LinearLayout>


                        </androidx.cardview.widget.CardView>
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/ll_secondcoureses"
                        android:layout_width="371dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/horizontal_dashboard"
                        android:layout_marginTop="@dimen/normalPadding"
                        android:layout_marginRight="@dimen/horizontal_dashboard"
                        android:gravity="center_horizontal"
                        android:orientation="horizontal"
                        android:weightSum="2">


                        <androidx.cardview.widget.CardView
                            android:id="@+id/profile"
                            android:layout_width="163dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"

                            android:layout_marginTop="20dp"
                            android:layout_marginRight="20dp"
                            android:layout_marginBottom="20dp"
                            android:background="@drawable/layout"
                            android:minHeight="150dp"
                            app:cardBackgroundColor="#FFF"
                            app:cardCornerRadius="16dp"
                            app:cardElevation="@dimen/card_itemcourse_elevation">

                            <!--    app:cardElevation="10dp"-->

                            <LinearLayout

                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:orientation="vertical">

                                <ImageView

                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:adjustViewBounds="true"
                                    android:cropToPadding="true"
                                    android:maxWidth="95dp"
                                    android:src="@drawable/profile1" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:layout_marginTop="10dp"

                                    android:layout_marginBottom="8dp"
                                    android:text="Profile"
                                    android:textSize="17sp"
                                    android:textStyle="bold" />

                            </LinearLayout>


                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:id="@+id/logout"
                            android:layout_width="163dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginTop="20dp"
                            android:layout_marginRight="20dp"
                            android:layout_marginBottom="20dp"
                            android:background="@drawable/layout"
                            android:minHeight="150dp"

                            app:cardCornerRadius="16dp"
                            app:cardElevation="@dimen/card_itemcourse_elevation">

                            <!--    app:cardElevation="10dp"-->

                            <LinearLayout
                                android:layout_width="137dp"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:orientation="vertical">

                                <ImageView
                                    android:id="@+id/button14"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"

                                    android:layout_marginLeft="28dp"
                                    android:adjustViewBounds="true"
                                    android:cropToPadding="true"
                                    android:maxWidth="95dp"
                                    android:src="@drawable/exit" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:layout_marginTop="10dp"

                                    android:layout_marginBottom="8dp"
                                    android:text="Log out"
                                    android:textSize="17sp"
                                    android:textStyle="bold" />

                            </LinearLayout>


                        </androidx.cardview.widget.CardView>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="345dp"
                        android:layout_height="120dp"
                        android:layout_marginLeft="@dimen/horizontal_dashboard"
                        android:layout_marginTop="3dp"
                        android:layout_marginRight="@dimen/horizontal_dashboard"
                        android:gravity="center_horizontal"
                        android:orientation="horizontal"
                        android:weightSum="2">
                        <androidx.cardview.widget.CardView
                            android:id="@+id/bmi"
                            android:layout_width="100dp"
                            android:layout_height="100dp"
                            android:layout_gravity="center"
                            android:layout_marginTop="9dp"
                            android:layout_marginRight="5dp"
                            android:layout_marginBottom="20dp"
                            android:minHeight="100dp"

                            app:cardCornerRadius="16dp"
                            app:cardElevation="@dimen/card_itemcourse_elevation">

                            <ImageView

                                android:layout_width="match_parent"
                                android:layout_height="70dp"
                                android:layout_gravity="center"
                                app:srcCompat="@drawable/image_12" />

                        </androidx.cardview.widget.CardView>


                    </LinearLayout>

                </LinearLayout>

            </RelativeLayout>

        </LinearLayout>




    </LinearLayout>





</ScrollView>
